function executeQuery($conn, $sql) {
    $result = $conn->query($sql);
    echo $result;

    if ($result === false) {
        echo "Error executing query: " . $conn->error;
        return;
    }

    if ($result->num_rows > 0) {
       

        // Print rows
        echo "success executing query: " ;
        while ($row = $result->fetch_assoc()) {foreach ($row as $value) {echo $value;} }

        

    } else {
        echo "No results found.";
    }
}

